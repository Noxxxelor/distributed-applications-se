@model InternshipApp.ViewModels.StudentVM

<h2>Create Student</h2>

<form method="post" action="/Student/Create">
    <div>
        <label>Full Name</label>
        <input type="text" name="FullName" value="@Model?.FullName" />
    </div>
    <div>
        <label>Email</label>
        <input type="email" name="Email" value="@Model?.Email" />
    </div>
    <div>
        <label>University</label>
        <input type="text" name="University" value="@Model?.University" />
    </div>
    <div>
        <label>Degree</label>
        <input type="text" name="Degree" value="@Model?.Degree" />
    </div>

    <button type="submit">Create</button>
</form>




